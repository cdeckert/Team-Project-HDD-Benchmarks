#ifndef SKIPPY_H_
#define SKIPPY_H_
#include <string>




class Skippy : public IBenchmark{
public:
	Skippy();
	Skippy(std::string aAddress);
	virtual void execute(int iteration);
	virtual ~Skippy();
private:
	int fd;
	std::string theAddress;
};


#endif /* SKIPPY_H_ */

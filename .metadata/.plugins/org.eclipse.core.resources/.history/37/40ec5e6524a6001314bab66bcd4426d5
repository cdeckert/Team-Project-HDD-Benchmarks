#include <iostream>

#include "Stopwatch.h"
#include "Skippy.h"

int main() {


	Stopwatch* stopwatch = new Stopwatch();

	stopwatch->start();
	Skippy* skippy = new Skippy("/dev/sdb");
	for(int i=0; i<10;i++)
	{
		std::cout << i<< "\n";
		skippy->execute(i);
		stopwatch->lap();
	}

	stopwatch->stop();

	return 0;
}

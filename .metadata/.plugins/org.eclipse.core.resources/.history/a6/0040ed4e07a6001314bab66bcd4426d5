#include <iostream>

#include "RealTimeClock.h"

int main() {


	double startcputime, endcputime, wcts, wcte;
	startcputime=RealTimeClock::getRealTime();

	std::cout << "\n"<< startcputime;

for(int i=0; i<2000;i++)
	{
		std::cout << ".";
	}
	std::cout << "\n"<< RealTimeClock::getRealTime()-startcputime;
	return 0;
}

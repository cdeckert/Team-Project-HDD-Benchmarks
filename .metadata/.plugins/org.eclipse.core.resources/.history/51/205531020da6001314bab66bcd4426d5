#include <iostream>

#include "Stopwatch.h"

int main() {


	Stopwatch stopwatch = Stopwatch::Stopwatch();

	stopwatch.start();
	for(int i=0; i<2000;i++)
	{
		std::cout << ".";
	}
	std::cout << "\n"<< RealTimeClock::getRealTime()-startcputime;

	stopwatch.end();
	return 0;
}
